      <div class="information">
        <h1 style="color: black;">Verify PAN</h1>
      </div>

      <!-- Create program starts here -->

      <html>

      <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Sign Up Form</title>
      </head>

      <body>

          <fieldset>
            <form action="/verifyPAN" method="post" enctype="multipart/form-data">
            <legend><span class="number">1</span> PAN info</legend>
            <div>
              <% if (validationErrors && validationErrors.PANNumber) { %>
                <div class="error-message">
                    <span><%= validationErrors.PANNumber.msg %></span>
                </div>
            <% } %> 
              <label for="PANNumber">PANNumber:</label>
              <input type="text" id="PANNumber" name="PANNumber" value="<%=fData.PANNumber %>">
            </div>
            <div>
              <% if (validationErrors && validationErrors.PANName) { %>
                <div class="error-message">
                    <span><%= validationErrors.PANName.msg %></span>
                </div>
            <% } %>
              <label for="PANName">PANName:</label>
              <input type="text" id="PANName" name="PANName" value="<%=fData.PANName %>">
            </div>
            <div>
            <label for="mail">PANImage:</label>
            <input type="file" class="form-control" id="PANPic" name="PANPic">
          </div>
          <div class="submitButton">
          <button type="submit" class="btn btn-success btn-small">Submit</button>
        </div>
          </fieldset>
        </form>

      </body>

      </html>


      <!-- Create program ends here -->

